{
    "configurations": [
        {
            "name": "raylib web",
            "includePath": [
                "${workspaceFolder}/**",
                "external/raylib-web/**",
                "/usr/lib/emscripten/system/**"
            ],
            "defines": ["PLATFORM_WEB"]
        },
        {
            "name": "raylib desktop",
            "includePath": [
                "${workspaceFolder}/**",
                "external/raylib-desktop/**",
                "/usr/lib/emscripten/system/**"
            ],
            "defines": ["PLATFORM_DESKTOP"]
        }
    ],
    "version": 4
}
